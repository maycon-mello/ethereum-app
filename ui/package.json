  "scripts": {
    "start": "node -r dotenv/config scripts/start.js",
    "build": "node -r dotenv/config scripts/build.js",
    "deploy": "gulp deploy",
    "compile-contracts": "node scripts/compile-contracts.js",
    "deploy-contracts": "node -r dotenv/config scripts/deploy-contracts",
    "generate-types": "node -r dotenv/config scripts/generate-redux-types",
    "test": "node scripts/test.js",
    "test:e2e": "node scripts/e2eTest.js",
    "test:e2e:watch": "yarn test:e2e --watch",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test:watch": "yarn test --watch --reporter nyan",
    "test:karma": "karma start ./config/karma/karma.conf.js --single-run",
    "test:karma:watch": "karma start ./config/karma/karma.conf.js",
    "coverage": "nyc -n src -c false --ignore-class-method render --check-coverage --branches 60 --statements 60 --lines 60 --functions 60 -e .tsx -e .ts -e .js npm run test",
    "coverage:watch": "watch 'yarn coverage' src",
    "eslint": "eslint --ext .js --ext .ts --ext .tsx src test config",
    "eslint:watch": "watch 'yarn eslint' src test config"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "@typescript-eslint",
      "chai-friendly"
    ],
    "overrides": {
        "files": [
            "**/*.ts",
            "**/*.tsx"
        ],
        "parser": "@typescript-eslint/parser",
        "rules": {
            "no-unused-vars": 0,
            "@typescript-eslint/no-unused-vars": "warn",
            "no-undef": "off"
        }
    },
    "rules": {
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "ramda",
      [
        "import",
        {
          "libraryName": "antd"
        }
      ]
    ]
  },
  "devDependencies": {
    "@types/puppeteer": "^1.12.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "aws-sdk": "^2.413.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-ramda": "^2.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "merge-stream": "^1.0.1",
    "winston": "^3.2.1"
  }
